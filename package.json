{
  "name": "ai-agent-router",
  "version": "0.1.0",
  "description": "A unified API gateway for managing multiple AI model providers (Anthropic, OpenAI, Gemini, etc.)",
  "main": "dist/src/cli/index.js",
  "bin": {
    "aar": "./dist/src/cli/index.js"
  },
  "scripts": {
    "dev": "next dev -p 9527",
    "build": "next build && tsc",
